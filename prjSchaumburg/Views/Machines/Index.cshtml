@model List<prjSchaumburg.Models.Domain.Machine>

@{
}

<h1>Machines</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Make</th>
            <th>Model</th>
            <th>Year</th>
            <th> </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var machine in Model)
        {
            <tr>
                <td>@machine.Id</td>
                <td>@machine.Make</td>
                <td>@machine.Model</td>
                <td>@machine.Year.ToString()</td>
                <td><a href="Machines/View/@machine.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
